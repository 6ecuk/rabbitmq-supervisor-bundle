{% extends 'SupervisorBundle::programm.conf.twig' %}

{% block cmd %}php {{ kernelRootDir }}/console {{ command }} --env={{ app.environment }}{% endblock cmd %}

{% block options %}
{% for name, value in options %}{{ name }}={{ value }}
{% endfor %}
stdout_logfile={{ workerOutputLog }}
stderr_logfile={{ workerErrorLog }}
{% endblock options %}
